@model IEnumerable<Domain.Models.Entities.Subject>
@using Domain.App_GlobalResources;

<div id="subjectList" class="display-label">
    <strong>
        @CommonResources.LblMySubjects:
    </strong>

    <table>
        <tr>
    @foreach (var subject in Model) {
            <td>
                <span class="label label-primary">@subject.SubjectName</span>
                <a href="@Url.Action("BySubject", new { subjectId = subject.Id })"><span class="label label-danger"><strong>x</strong></span></a>
            </td>
    }
        </tr>
    </table>
</div>
<hr />
