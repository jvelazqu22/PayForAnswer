@model Domain.Models.Helper.TestModel
<!DOCTYPE html>
<html lang="en"><head>
<meta http-equiv="content-type" content="text/html; charset=UTF-8">
<title>Example of Twitter Bootstrap Popover</title>
<link rel="stylesheet" type="text/css" href="~/Content/bootstrap.min.css">
<script type="text/javascript" src="http://code.jquery.com/jquery.js"></script>
<script type="text/javascript" src="~/Scripts/bootstrap.min.js"></script>
<script type="text/javascript" src="~/Scripts/Custom/CreateQuestion.js"></script>
<script type="text/javascript">
    $('document').ready(function () {
        //document.getElementById("includedContent").innerHTML = '<object type="text/html" data="http://payforanswer.blob.core.windows.net/comments/1/comment.html" ></object>';
        //$("#includedContent").load("test.html");
        //$("#includedContent").html('<object data="http://payforanswer.blob.core.windows.net/comments/1/comment.html">');
        LoadComments("@Model.CommentsUrl");
    });

    $(function () {
        $(".popover-examples a").popover({
            placement: 'top'
        });

        $(".tooltip-examples a").tooltip({
            placement : 'top'
        });

      $('#typeahead').typeahead({
            source: function (term, process) {
                //var url = '@Url.Content("~/Test/GetNames")';
                var url = '@Url.Content("~/Test/Autocomplete")';

                return $.getJSON(url, { term: term }, function (data) {
                    return process(data);
                });
            }
      });

    });
</script>
<style type="text/css">
	.bs-example{
    	margin: 150px 50px;
    }
</style>
</head>
<body>

    <input type="text" name="names" value="" id="typeahead" data-provide="typeahead" />

    <div class="bs-example"> 
        <ul class="popover-examples inline">
            <li><a href="#" class="btn" data-toggle="popover" title="Popover title" data-content="Default popover">Popover</a></li>
            <li><a href="#" class="btn btn-primary" data-toggle="popover" title="Popover title" data-content="Another popover">Another popover</a></li>
            <li><a href="#" class="btn btn-success" data-toggle="popover" title="Popover title" data-content="A much longer popover to demonstrate the max-width of the Bootstrp popover.">Large popover</a></li>
            <li><a href="#" class="btn btn-warning" data-toggle="popover" title="Popover title" data-content="The last tip!">Last popover</a></li>
        </ul>
    </div>

    <div class="bs-example"> 
        <ul class="tooltip-examples inline">
            <li><a href="#" data-toggle="tooltip" data-original-title="Default tooltip">Tooltip</a></li>
            <li><a href="#" data-toggle="tooltip" data-original-title="Another tooltip">Another tooltip</a></li>
            <li><a href="#" data-toggle="tooltip" data-original-title="A much longer tooltip to demonstrate the max-width of the Bootstrp tooltip.">Large tooltip</a></li>
            <li><a href="#" data-toggle="tooltip" data-original-title="The last tip!">Last tooltip</a></li>
        </ul>
    </div>

    <div id="includedContent"></div>

</body>
</html>                                		
