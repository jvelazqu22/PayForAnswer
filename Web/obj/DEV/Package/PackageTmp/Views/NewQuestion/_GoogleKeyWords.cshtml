@model Domain.Models.CreateQuestionViewModel
@using Domain.App_GlobalResources;
@using Domain.Constants;

<strong>
    @CommonResources.GoogleSearchKeywords
    <a href="#" data-toggle="tooltip" title="@CommonResources.GoogleSearchKeywordsDescr">@CommonResources.QuestionMarkLinkSymbol</a>
</strong>
@Html.Raw(CommonResources.GoogleSearchKeywordsDescr)

<div class="editor-field">
    @Html.TextBoxFor(model => model.GoogleSearchKeywords1, new { @class = "form-control", placeholder = CommonResources.GoogleSearchKeywordsPlaceHolder1, maxLength = Size.GoogleSearchKeyWordMaxCharacters })
</div>
@Html.ValidationMessageFor(model => model.GoogleSearchKeywords1, "", new { @class = "text-danger" })
