@model PagedList.IPagedList<Domain.Models.Entities.Question>
@using PagedList.Mvc
@using Domain.App_GlobalResources;

<h2>@ViewBag.Title</h2>

@if (Model.Count() == 0)
{
    <div class="text-center">
        <p class="bg-primary">@CommonResources.NoQuestionsOrProjectsOpen</p>
    </div>
}
else
{
    @Html.Partial("~/Views/Shared/_Questions.cshtml", Model)
}

@*@Html.PagedListPager( Model, page => Url.Action(ViewBag.MethodName, new { page }) )*@